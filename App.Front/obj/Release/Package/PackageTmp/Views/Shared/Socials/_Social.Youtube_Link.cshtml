<!----_Social.Youtube_Link-->
<span id="divYoutubeLink"></span>


<script>
    jQuery(function () {
        'use strict';

        jQuery.post('@Url.Action("GetSettingSeo", "Summary")')
            .done(function (data) {
                var youtubeLink = data.response.YoutubeLink;
                if (youtubeLink != null) {
                    var code = '<a href="' + youtubeLink + '" class=""><i class="fa fa-youtube-square"></i></a>';

                    jQuery("#divYoutubeLink").html(code);
                }
             });
        })

</script>
<!----/_Social.Youtube_Link-->