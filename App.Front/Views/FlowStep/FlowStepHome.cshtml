<!---FlowStepHome-->
@using App.Aplication
@model IEnumerable<App.Domain.Entities.Data.FlowStep>

@if (Model.IsAny())
{
    <div class="row row-fluid">
        <div class="col-sm-12">
            <div class="vc_column-inner ">
                <div class="wpb_wrapper">
                    <div class="houzez-module module-title section-title-module text-center ">
                        <h2>@T("ThanhVien")</h2>
                    </div>

                    <!--start agents module-->

                    <div id="agents-module" class="houzez-module agents-module">
                        <div class="agents-blocks-main">
                            <div class="row no-margin">

                                @foreach (var item in Model)
                                {
                                    <div class="col-md-3 col-sm-6">
                                        <div class="agents-block">

                                            <figure class="auther-thumb">
                                                <a href="#" class="view">
                                                    <img src="@Url.Content("~/" + item.ImageUrl)" class="img-circle" width="150" height="150" alt="@item.Title">
                                                </a>
                                            </figure>


                                            <div class="web-logo text-center">
                                            </div>

                                            <div class="block-body">
                                                <p class="auther-info">
                                                    <span class="blue">@item.Title</span>
                                                    
                                                </p>
                                                <div class="description">@Html.Raw(item.Description)</div>
                                              
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>

                    <!--end post agents module-->


                </div>
            </div>
        </div>
    </div>
}

<!--/-FlowStepHome-->