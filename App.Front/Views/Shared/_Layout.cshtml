@using App.Aplication
<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <title>@ViewBag.Title </title>
    <meta name="keywords" content="@ViewBag.KeyWords" />
    <meta name="description" content="@ViewBag.Description" />
    <meta name="generator" content="" />
    <link rel="canonical" href="@Html.Raw(Request.Url.AbsoluteUri)" />
    <link rel="alternate" hreflang="vi_vn" href="@ViewBag.SiteUrl" />
    <meta name="robots" content="index, follow, noodp" />
    <meta name="googlebot" content="index,archive,follow,noodp" />
    <meta name="msnbot" content="all,index,follow" />
    <meta name="copyright" content="@Utils.CurrentHost" />
    <meta property="og:locale" content="vi_VN">
    <meta property="og:site_name" content="@ViewBag.Title" />
    <meta property="og:title" content="@ViewBag.Title" />
    <meta property="og:description" content="@ViewBag.Description" />

    <!-- You can use Open Graph tags to customize link previews.-->
    <meta property="og:url" content="@(string.Concat(Utils.CurrentHost,ViewBag.Image))">
    <meta property="og:type" content="website" />
    <meta property="og:image" content="@(string.Concat(Utils.CurrentHost,ViewBag.Image))" />
    <link href="@ViewBag.Favicon" rel="shortcut icon" type="image/x-icon" />

    @{Html.RenderAction("GetMeta", "Summary"); }
    @{Html.RenderAction("GetMetaTagsSeo", "Summary");}

    @{Html.RenderPartial("_StyleSheet");}

    @RenderSection("stylesheets", false)
    @RenderSection("styles", false)

    @{Html.RenderPartial("_Scripts"); }

</head>
